<p id="notice"><%= notice %></p>

<h1>Listing Student Logs</h1>

<table>
  <thead>
    <tr>
      <th>Branch</th>
      <th>Student</th>
      <th>Sequence no</th>
      <th>Log entry date</th>
      <th>Log entry contents</th>
      <th>Log engry person</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_logs.each do |student_log| %>
      <tr>
        <td><%= student_log.branch_id %></td>
        <td><%= student_log.student_id %></td>
        <td><%= student_log.sequence_no %></td>
        <td><%= student_log.log_entry_date %></td>
        <td><%= student_log.log_entry_contents %></td>
        <td><%= student_log.log_engry_person %></td>
        <td><%= link_to 'Show', student_log %></td>
        <td><%= link_to 'Edit', edit_student_log_path(student_log) %></td>
        <td><%= link_to 'Destroy', student_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student log', new_student_log_path %>
