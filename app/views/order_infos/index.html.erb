<p id="notice"><%= notice %></p>

<h1>Listing Order Infos</h1>

<table>
  <thead>
    <tr>
      <th>Application</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_infos.each do |order_info| %>
      <tr>
        <td><%= order_info.application_id %></td>
        <td><%= link_to 'Show', order_info %></td>
        <td><%= link_to 'Edit', edit_order_info_path(order_info) %></td>
        <td><%= link_to 'Destroy', order_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order info', new_order_info_path %>
