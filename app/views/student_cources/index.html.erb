<p id="notice"><%= notice %></p>

<h1>Listing Student Cources</h1>

<table>
  <thead>
    <tr>
      <th>Branch</th>
      <th>Student</th>
      <th>Sequence no</th>
      <th>Coruse</th>
      <th>Lecture startdate</th>
      <th>Lecture enddate</th>
      <th>Timetable week</th>
      <th>Timetabel time</th>
      <th>Class</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_cources.each do |student_cource| %>
      <tr>
        <td><%= student_cource.branch_id %></td>
        <td><%= student_cource.student_id %></td>
        <td><%= student_cource.sequence_no %></td>
        <td><%= student_cource.coruse_id %></td>
        <td><%= student_cource.lecture_startdate %></td>
        <td><%= student_cource.lecture_enddate %></td>
        <td><%= student_cource.timetable_week %></td>
        <td><%= student_cource.timetabel_time %></td>
        <td><%= student_cource.class %></td>
        <td><%= link_to 'Show', student_cource %></td>
        <td><%= link_to 'Edit', edit_student_cource_path(student_cource) %></td>
        <td><%= link_to 'Destroy', student_cource, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student cource', new_student_cource_path %>
